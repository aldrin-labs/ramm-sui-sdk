{
    "name": "@ramm/ramm-sui-sdk",
    "version": "1.0.0",
    "description": "TypeScript SDK for the Sui Move RAMM",
    "main": "index.ts",
    "type": "module",
    "scripts": {
        "build:sdk": "tsup ./index.ts --format cjs,esm --dts --clean",
        "test": "vitest"
    },
    "publishConfig": {
        "registry": "https://gitlab.com/api/v4/projects/54281288/packages/npm/"
      },
    "repository": "https://github.com/aldrin-labs/ramm-sui-sdk.git",
    "author": "Alexandre Bald√© <alexandre@aldrin.com>",
    "license": "AGPL-3.0",
    "private": false,
    "dependencies": {
        "@mysten/sui.js": "^0.49.1",
        "dotenv": "^16.3.2"
    },
    "compilerOptions": {
        "module": "node16",
        "target": "es2023",
        "strict": true,
        "verbatimModuleSyntax": true,
        "declaration": true,
        "sourceMap": true,
        "declarationMap": true
    },
    "devDependencies": {
        "@types/node": "^20.11.5",
        "tsup": "^8.0.1",
        "vitest": "^1.2.1"
    }
}
